{
  "document": {
    "type": "APL",
    "version": "1.0",
    "theme": "dark",
    "import": [
      {
        "name": "alexa-viewport-profiles",
        "version": "1.0.0"
      },
      {
        "name": "alexa-layouts",
        "version": "1.0.0"
      },
      {
        "name": "alexa-styles",
        "version": "1.0.0"
      }
    ],
    "resources": [
      {
        "description": "Stock color for the light theme",
        "colors": {
          "colorTextPrimary": "#151920"
        }
      },
      {
        "description": "Stock color for the dark theme",
        "when": "${viewport.theme == 'dark'}",
        "colors": {
          "colorTextPrimary": "#f0f1ef"
        }
      },
      {
        "description": "Standard font sizes",
        "dimensions": {
          "textSizeBody": 44,
          "textSizePrimary": 24,
          "textSizePrimary2": 30,
          "textSizePrimary3": 36,
          "textSizeSecondary": 23,
          "textSizeSecondaryHint": 25
        }
      },
      {
        "description": "Common spacing values",
        "dimensions": {
          "spacingThin": 6,
          "spacingSmall": 12,
          "spacingMedium": 24,
          "spacingLarge": 48,
          "spacingExtraLarge": 72
        }
      },
      {
        "description": "Common margins and padding",
        "dimensions": {
          "marginTop": 40,
          "marginLeft": 60,
          "marginRight": 60,
          "marginBottom": 40
        }
      }
    ],
    "styles": {
      "textStyleBase": {
        "description": "Base font description; set color and core font family",
        "values": [
          {
            "color": "@colorTextPrimary",
            "fontFamily": "Amazon Ember"
          }
        ]
      },
      "textStyleBase0": {
        "description": "Thin version of basic font",
        "extend": "textStyleBase",
        "values": {
          "fontWeight": "100"
        }
      },
      "textStyleBase1": {
        "description": "Light version of basic font",
        "extend": "textStyleBase",
        "values": {
          "fontWeight": "300"
        }
      },
      "mixinBody": {
        "values": {
          "fontSize": "@textSizeBody"
        }
      },
      "mixinPrimary": {
        "values": {
          "fontSize": "@textSizePrimary"
        }
      },
      "mixinPrimary2": {
        "values": {
          "fontSize": "@textSizePrimary2"
        }
      },
      "mixinPrimary3": {
        "values": {
          "fontSize": "@textSizePrimary3"
        }
      },
      "mixinSecondary": {
        "values": {
          "fontSize": "@textSizeSecondary"
        }
      },
      "textStylePrimary": {
        "extend": [
          "textStyleBase1",
          "mixinPrimary"
        ]
      },
      "textStylePrimary2": {
        "extend": [
          "textStyleBase1",
          "mixinPrimary2"
        ]
      },
      "textStyleSecondary": {
        "extend": [
          "textStyleBase0",
          "mixinSecondary"
        ]
      },
      "textStyleSecondary2": {
        "extend": [
          "textStyleBase1",
          "mixinSecondary"
        ]
      },
      "textStyleBody": {
        "extend": [
          "textStyleBase1",
          "mixinBody"
        ]
      },
      "textStyleSecondaryHint": {
        "values": {
          "fontFamily": "Bookerly",
          "fontStyle": "italic",
          "fontSize": "@textSizeSecondaryHint",
          "color": "@colorTextPrimary"
        }
      }
    },
